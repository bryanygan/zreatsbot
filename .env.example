# Copy this file to .env and fill in your values

# Required
BOT_TOKEN=

# Authorization - Use either AUTHORIZED_USER_IDS or OWNER_ID
# AUTHORIZED_USER_IDS: Comma-separated list of Discord user IDs who can use admin commands
# Example: AUTHORIZED_USER_IDS=123456789012345678,987654321098765432
AUTHORIZED_USER_IDS=

# Legacy support - if AUTHORIZED_USER_IDS is not set, falls back to OWNER_ID
OWNER_ID=

# Optional - comment out what you don't use
POINTS_CHANNEL_ID=
OPENER_CHANNEL_ID=
ROLE_PING_ID=1352022044614590494
VIP_ROLE_ID=1371247728646033550
ORDER_CHANNEL_MENTION=<#1350935337269985334>

# Status Monitoring (optional - for bot status webpage)
# Generate a secure API key with: python -c "import secrets; print(secrets.token_urlsafe(32))"
STATUS_API_PORT=5000
STATUS_API_HOST=0.0.0.0
STATUS_API_KEY=your-secure-api-key-here-change-this
CORS_ORIGINS=https://yourportfolio.com,http://localhost:4321
